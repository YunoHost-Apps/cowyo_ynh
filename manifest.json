{
    "name": "Cowyo",
    "id": "cowyo",
    "packaging_format": 1,
    "description": {
        "en": "Feature-rich wiki webserver for minimalists",
        "fr": "Serveur de wiki minimaliste mais riche en fonctionnalitÃ©s"
    },
    "version": "2.12.0~ynh5",
    "url": "https://github.com/schollz/cowyo",
    "upstream": {
        "license": "MIT",
        "demo": "https://cowyo.com",
        "admindoc": "https://github.com/schollz/cowyo/blob/master/README.md",
        "code": "https://github.com/schollz/cowyo"
    },
    "license": "MIT",
    "maintainer": {
        "name": "tituspijean",
        "email": "tituspijean@outlook.com"
    },
    "requirements": {
        "yunohost": ">= 11.0.9"
    },
    "multi_instance": true,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install": [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
}
