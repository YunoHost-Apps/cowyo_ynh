{
    "name": "Cowyo",
    "id": "cowyo",
    "packaging_format": 1,
    "description": {
        "en": "Feature-rich wiki webserver for minimalists",
        "fr": "Serveur de wiki minimaliste mais riche en fonctionnalités."
    },
    "version": "2.12.0~ynh4",
    "url": "https://github.com/schollz/cowyo",
    "license": "MIT",
    "maintainer": {
        "name": "tituspijean",
        "email": "tituspijean@outlook.com"
    },
    "requirements": {
        "yunohost": ">= 3.8.2"
    },
    "multi_instance": true,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install": [{
                "name": "domain",
                "type": "domain",
                "ask": {
                    "en": "Choose a domain name dedicated to Cowyo",
                    "fr": "Choisissez un nom de domaine dédié à Cowyo"
                },
                "example": "example.com"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "ask": {
                    "en": "Is it a public application?",
                    "fr": "Est-ce une application publique ?"
                },
                "help": {
                    "en": "If public, anyone will be able to create pages in Cowyo",
                    "fr": "Si publique, quiconque pourra créer des pages dans Cowyo"
                },
                "default": true
            }
        ]
    }
}
