{
    "name": "Cowyo",
    "id": "cowyo",
    "packaging_format": 1,
    "description": {
        "en": "A feature-rich wiki webserver for minimalists, packaged for YunoHost"
    },
    "version": "2.12.0~ynh1",
    "url": "https://github.com/schollz/cowyo",
    "license": "MIT",
    "maintainer": {
        "name": "tituspijean",
        "email": "tituspijean@outlook.com"
    },
    "requirements": {
        "yunohost": ">= 2.7.14"
    },
    "multi_instance": true,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install": [{
                "name": "domain",
                "type": "domain",
                "ask": {
                    "en": "Choose a domain name dedicated to cowyo",
                    "fr": "Choisissez un nom de domaine dédié à cowyo"
                },
                "example": "example.com"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "ask": {
                    "en": "Is it a public application?",
                    "fr": "Est-ce une application publique ?"
                },
                "default": true
            }
        ]
    }
}